{
    "name": "testimony/testify",
    "description": "A flexible framework that allows you to test pieces of PHP code or program from a CLI",
    "license": "Apache-2.0",
    "keywords": [
        "testimony",
        "testify",
        "framework",
        "sikessem",
        "tester",
        "test",
        "cli",
        "php"
    ],
    "authors": [{
        "homepage": "https://foss.sikessem.com",
        "name": "SIGUI KessÃ© Emmanuel",
        "email": "developer@sikessem.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/SIKessEm/Testimony",
        "issues": "https://github.com/SIKessEm/Testimony/issues"
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
    "autoload": {
        "files": [
            "ext/testimony.php"
        ],
        "psr-4": {
          "Testimony\\": "src/main/"
        }
    },
    "homepage": "https://github.com/SIKessEm/Testimony#readme",
    "config": {
        "vendor-dir": "pkg",
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": "cli/testify",
    "minimum-stability": "dev",
    "prefer-stable": true
}
